    "lint:fix": "npm run lint -- --fix",
    "build": "npm run lint && tsc --build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
