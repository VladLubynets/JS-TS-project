{
  "name": "simple",
  "version": "1.0.0",
  "description": "ts simple project",
  "main": "index.js",
  "scripts": {
    "test": "npx eslint . && vitest",
    "test:hw": "powershell -Command \"$env:SKIP_GLOBAL_SETUP='1'; npx vitest hw/tests\"",
    "test:hw:run": "powershell -Command \"$env:SKIP_GLOBAL_SETUP='1'; npx vitest --run hw/tests\"",
