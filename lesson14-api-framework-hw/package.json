    "@types/chai": "^5.2.3",
    "@types/dotenv-safe": "^9.1.0",
    "chai": "^6.2.0",
    "dotenv-safe": "^5.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.5"
  },
